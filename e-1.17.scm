(define (m a b)
  (mult 0 a b)
  )
(define (mult x a b)
  (cond ((= b 0) x)
        ((even? b) (mult x (double a) (halve b)))
        (else (mult (+ x a) a (- b 1)))
        )
  )
(define (double a)
  (+ a a)
  )
(define (halve b)
  (/ b 2)
  )

(define (even? b)
  (= (remainder b 2) 0)
  )

(m 2 9)
(m 3 9)
(m 30 9)
(m 3 1)
(m 2 256)
